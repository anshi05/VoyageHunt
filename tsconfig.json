{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react"  // or "react-jsx" if you're using React 17 or higher
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.jsx",
    "app/_layout.jsx",
    "app/modal.jsx"
, "app/(tabs)/nearby.jsx", "app/(tabs)/index.jsx", "app/(tabs)/place.jsx", "app/(tabs)/hunt.jsx", "app/(tabs)/_layout.tsx"  ]
}
